{
  "author": "Niall Bourke",
  "cite": "SynthSeg: Domain Randomisation for Segmentation of Brain MRI Scans of any Contrast and Resolution, B. Billot, D.N. Greve, O. Puonti, A. Thielscher, K. Van Leemput, B. Fischl, A.V. Dalca, J.E. Iglesias: https://github.com/BBillot/SynthSeg ",
  "command": "python3 /flywheel/v0/run.py",
  "config": {
    "parc": {
      "default": true,
      "description": "(optional) Whether to perform cortex parcellation.",
      "type": "boolean"
    },
    "QC": {
      "default": true,
      "description": "(optional) Output CSV file with qc scores for all subjects.",
      "type": "boolean"
    },
    "robust": {
      "default": true,
      "description": "(optional) Whether to use robust predictions (slower)",
      "type": "boolean"
    },
    "vol": {
      "default": true,
      "description": "(optional) Output CSV file with volumes for all structures and subjects.",
      "type": "boolean"
    },
    "infant": {
      "default": false,
      "description": "BETA: Use infant model. This will override the default model with an infant model trained on UCT data.",
      "type": "boolean"
    }
  },
  "custom": {
    "analysis-level": "participant",
    "flywheel": {
      "classification": {
        "components": [
          "FreeSurfer"
        ],
        "keywords": [
          "segmentation",
          "volume",
          "template"
        ],
        "modality": [
          "MR"
        ],
        "type": [
          "nifti"
        ]
      },
      "suite": "UNITY Apps"
    },
    "gear-builder": {
      "category": "analysis",
      "image": "flywheel/synthseg:0.2.6"
    },
    "kcl-app-binary": "app/main.sh",
    "kcl-app-modalities": [
      "anat"
    ],
    "license": {
      "dependencies": [
        {
          "name": "Other",
          "url": "/usr/local/freesurfer/license.txt"
        }
      ],
      "main": {
        "name": "",
        "url": ""
      },
      "non-commercial-use-only": false
    }
  },
  "description": "This gear implements out-of-the-box SynthSeg, the first convolutional neural network for segmentation of brain MRI scans of any contrast and resolution. Additional infant models have been provided by Henry Tregidgo. These models are based on the original SynthSeg model, but have been trained on infant data from UCT.",
  "environment": {
    "CONDA_DIR": "/opt/conda",
    "FLYWHEEL": "/flywheel/v0",
    "GPG_KEY": "E3FF2839C048B25C084DEBE9B26995E310250568",
    "LANG": "C.UTF-8",
    "PATH": "/flywheel/v0/app/infant:/opt/conda/bin:/usr/local/freesurfer/bin:/usr/local/freesurfer/fsfast/bin:/usr/local/freesurfer/tktools:/usr/local/freesurfer/mni/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin",
    "PYTHON_GET_PIP_SHA256": "fa6f3fb93cce234cd4e8dd2beb54a51ab9c247653b52855a48dd44e6b21ff28b",
    "PYTHON_GET_PIP_URL": "https://github.com/pypa/get-pip/raw/c20b0cfd643cd4a19246ccf204e2997af70f6b21/public/get-pip.py",
    "PYTHON_PIP_VERSION": "21.2.3",
    "PYTHON_VERSION": "3.8.11",
    "FREESURFER": "/usr/local/freesurfer",
    "FREESURFER_HOME": "/usr/local/freesurfer",
    "container": "oci",
    "PWD": "/flywheel/v0",
    "SHLVL": "0",
    "_": "/usr/bin/printenv",
    "FSLDIR": "/opt/conda"
  },
  "inputs": {
    "api-key": {
      "base": "api-key",
      "read-only": true
    },
    "input": {
      "base": "file",
      "description": "input file (usually isotropic reconstruction)",
      "optional": false
    }
  },
  "label": "SynthSeg",
  "license": "MIT",
  "maintainer": "Niall Bourke <niall.bourke@kcl.ac.uk>",
  "name": "synthseg",
  "source": "https://github.com/BBillot/SynthSeg ",
